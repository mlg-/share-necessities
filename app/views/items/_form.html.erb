<%= form_for [@organization, @item] do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :quantity %>
  <%= f.number_field :quantity %>

  <%= f.label :url %>
  <%= f.text_field :url %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.submit class: "button" %>

<% end %>
