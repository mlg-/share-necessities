<div class="row">
  <div class="small-11 small-offset-1 medium-7 medium-offset-3 columns">
    <h1>All Organizations</h1>
    <ol>
      <% @organizations.each do |org| %>
        <li><%= link_to org.name, organization_path(org.id) %></li>
      <% end %>
    </ol>
      <%= paginate @organizations %>
  </div>
</div>